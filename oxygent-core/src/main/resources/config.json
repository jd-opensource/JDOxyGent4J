{
    "default": {
        "app": {
            "name": "app",
            "version": "1.0.0",
            "biz_type": "oxygent",
            "scan_oxygent_path": "com.jd.oxygent"
        },
        "llm": {
            "cls": "com.jd.oxygent.core.oxygent.oxy.llms.HttpLlm",
            "base_url": "http://localhost:11434",
            "api_key": "YOUR_API_KEY",
            "model_name": "gpt-4.1",
            "temperature": 0.1,
            "max_tokens": 4096, 
            "top_p": 1
        },
        "xfile": {
          "save_dir": "./cache_dir/xfile"
        },
        "message": {
            "send_tool_call": true,
            "detailed_tool_call": true,
            "send_observation": true,
            "detailed_observation": true,
            "send_think": true,
            "send_answer": true,
            "stored": true,
            "show_in_terminal": true,
            "send_full_arguments": false
        },
        "vearch": {},
        "es": {
          "type": "local",
          "hosts": [
            "${ES_HOST_1}",
            "${ES_HOST_2}",
            "${ES_HOST_3}"
          ],
          "user": "${ES_TEST_USER}",
          "password": "${ES_TEST_PASSWORD}"
        },
        "redis": {
          "host": "127.0.0.1",
          "port": 6379,
          "database": 0,
          "password": "RedisOxyGentTeam",
          "timeout": 5000
        },
        "server": {
            "host": "127.0.0.1",
            "port": 8080,
            "auto_open_webpage": true,
            "log_level": "INFO"
        },
        "agent": {
            "prompt": "",
            "llm_model": "default_llm",
            "input_schema": {
                "properties": {"query": {"description": "Query question"}},
                "required": ["query"]
            }
        },
        "tool": {
            "mcp_is_keep_alive": true, 
            "is_concurrent_init": true
        }
    },
    "dev": {},
    "test": {},
    "prod": {}
}